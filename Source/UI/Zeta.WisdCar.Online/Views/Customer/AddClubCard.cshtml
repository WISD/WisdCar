@model Zeta.WisdCar.Model.VO.ClubCardVO

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>AddClubCard</title>
</head>
<body>
    <h4 class="modal-title" id="divtitle">会员卡开卡</h4>
    <div> 
        @using (Html.BeginForm("", "", FormMethod.Post, new { @class = "form-horizontal", id = "dilogform", role = "form" }))
        {
            <div class="form-group">
                <label class="col-md-4 control-label">姓名:<span class="required">*</span></label>
                <div class="col-md-8">
                    @Html.TextBoxFor(m=>m.CustName, new { @class = "form-control" })
                   
                    @*<input type="text" class="form-control" id="txtcusname" name="cusname" placeholder="">*@
                </div>
            </div>

          

            <div class="form-group">
                <label class="col-md-4 control-label">手机号:<span class="required">*</span></label>
                <div class="col-md-8">
                    @Html.TextBoxFor(m => m.MobileNo, new { @class = "form-control" })
                    @*<input type="text" class="form-control" id="txtcuscid" name="cuscid" placeholder="">*@
                    @Html.HiddenFor(m => m.CustomerID)
                </div>
            </div>
             <div class="form-group">
                <label class="col-md-4 control-label">会员卡卡号:<span class="required">*</span></label>
                <div class="col-md-8">
                    @Html.TextBoxFor(m => m.ClubCardNo, new { @class = "form-control" })
                    @*<input type="text" class="form-control" id="txtcuscid" name="cuscid" placeholder="">*@
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-4 control-label">会员卡类型:<span class="required">*</span></label>
                <div class="col-md-8">
                    <select class="table-group-action-input form-control input-medium" name="ClubCardTypeName" id="txtcardtype">

                        <option value="白金卡" selected>白金卡</option>
                        <option value="金卡">金卡</option>
                    </select>
                </div>
            </div>


           

        }
      
    </div>
</body>
</html>
@section script{
    <script>
        function getValidate() {
            alert(4);
            return false;
        }
    </script>
}
